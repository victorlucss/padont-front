{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "styles": "./styles",
          "@": "./",
          "@Components": "./components"
        },
        "cwd": "babelrc"
      }
    ]
  ]
}